      .alerts-grid {
        display: grid;
        grid-template-columns: 1fr 1fr;
        gap: 20px;
      }

      .alert-item {
        background-color: rgba(255, 255, 255, 0.1);
        border: 1px solid rgba(255, 255, 255, 0.15);
        box-shadow: 0px 4px 9px 0px rgba(0, 0, 0, 0.05);
        -webkit-backdrop-filter: blur(2px);
        backdrop-filter: blur(2px);
        border-radius: 11px;
        overflow: hidden;
        transition: background-color 0.3s ease, border-color 0.3s ease;
        padding: 80px 20px 20px;
      }

      .alert-item:hover {
        background-color: rgba(255, 255, 255, 0.2);
      }

      .alert-item .badge-status {
        position: absolute;
        left: 20px;
        top: 20px;
        padding: 5px 10px;
        font-size: 16px !important;
        display: inline-block;
        line-height: 20px;
        border-radius: 6px;
        border: 1px solid rgba(255, 255, 255, 0.15);
        background: rgba(124, 255, 239, 0.35);
        background-color: #418e9f;
        box-shadow: 0px 4px 9.3px 0px rgba(0, 0, 0, 0.05);
        -webkit-backdrop-filter: blur(6.25px);
        backdrop-filter: blur(6.25px);
      }

      .alert-item .alert-name {
        font-family: "Avenir", Sans-serif;
        font-size: 20px;
        font-weight: 700;
        color: var(--e-global-color-text);
      }

      .alert-item .alert-date {
        position: absolute;
        right: 20px;
        top: 20px;
      }

      .alerts-pagination {
        text-align: center;
        margin-top: 2rem;
      }
      .alerts-pagination ul {
        display: inline-flex;
        list-style: none;
        gap: 10px;
        padding: 0;
      }
      .alerts-pagination a,
      .alerts-pagination span {
        padding: 6px 12px;
        color: #fff;
        text-decoration: none;
        border-radius: 4px;
      }
      .alerts-pagination .current {
        background: #fff;
        color: #000;
        border-color: #fff;
      }

